    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>

        <PackageId>OatmealDome.UnifiedDiff</PackageId>
        <Version>1.0.0</Version>
        <Authors>OatmealDome</Authors>
